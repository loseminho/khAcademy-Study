<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

	<!-- 
		비어있는 Member객체 생성 후 setter로 값 대입
		Mmeber member1 = new Member();
		member1.setName("이윤수");
		member1.setAge(20);
		member1.setAddr("서울시 영등포구");
	 -->
	<bean id="member1" class="kr.or.iei.vo.Member">
		<property name="name" value="이윤수"/>
		<property name="age">
			<value type="java.lang.Integer">20</value>
		</property>
		<property name="addr" value="서울시 영등포구"/>
	</bean>
	
	<!-- 
		생성자를 이용해서 객체를 생성하며 데이터를 넣기
		Member member2 = new Member("이윤수1",30,"부산");
	 -->
	 <bean id="member2" class="kr.or.iei.vo.Member">
	 
	 </bean>
</beans>
