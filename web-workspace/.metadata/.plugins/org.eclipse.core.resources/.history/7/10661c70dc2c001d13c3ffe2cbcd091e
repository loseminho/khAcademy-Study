package kr.or.iei.notice.model.vo;

import java.util.ArrayList;

public class NoticeViewData {
	private Notice n;
	private ArrayList<NoticeComment> commnetList;
	private ArrayList<NoticeComment> reCommentList;
	
	public NoticeViewData() {
		super();
	}
	
	public NoticeViewData(Notice n, ArrayList<NoticeComment> commnetList, ArrayList<NoticeComment> reCommentList) {
		super();
		this.n = n;
		this.commnetList = commnetList;
		this.reCommentList = reCommentList;
	}

	public Notice getN() {
		return n;
	}
	public void setN(Notice n) {
		this.n = n;
	}
	public ArrayList<NoticeComment> getCommnetList() {
		return commnetList;
	}
	public void setCommnetList(ArrayList<NoticeComment> commnetList) {
		this.commnetList = commnetList;
	}
	public ArrayList<NoticeComment> getReCommentList() {
		return reCommentList;
	}
	public void setReCommentList(ArrayList<NoticeComment> reCommentList) {
		this.reCommentList = reCommentList;
	}
	
}
